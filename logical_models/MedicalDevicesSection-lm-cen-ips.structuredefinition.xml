<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ../../../__FHIRr4Tools/fhir-all-xsd/structuredefinition.xsd">
	<id value="MedicalDevicesSection-lm-cen-ips"/>
	<url value="http://hl7.org/fhir/uv/ips/StructureDefinition/MedicalDevicesSection-lm-cen-ips"/>
	<name value="MedicalDevicesSection-lm-cen-ips"/>
	<title value="MedicalDevices Section (EN 17269)"/>
	<status value="draft"/>
	<description value="Devices that are implanted in the patient and external medical devices and equipment that the health status depends on."/>
	<purpose value="To inform the reader that the patient has internal and/or external devices, and these may need to be taken into account in any planned treatment or intervention."/>
	<kind value="logical"/>
	<abstract value="true"/>
	<type value="Element"/>
	<baseDefinition value="http://hl7.org/fhir/StructureDefinition/Element"/>
	<derivation value="specialization"/>
	<differential>
		<element id="MedicalDevicesSection">
			<path value="MedicalDevicesSection"/>
			<short value="MedicalDevices Section"/>
			<definition value="Devices that are implanted in the patient and external medical devices and equipment that the health status depends on."/>
			<min value="0"/>
			<max value="*"/>
			<type>
				<code value="Section"/>
			</type>
			<constraint>
				<key value="br-md-1"/>
				<severity value="error"/>
				<human value="If content status positive then Device list SHALL be non-empty AND the content status SHALL be omitted."/>
			</constraint>
			<constraint>
				<key value="br-md-2"/>
				<severity value="error"/>
				<human value="If content status records absence then Device List SHALL be omitted."/>
			</constraint>
		</element>
		<element id="MedicalDevicesSection.ContentStatus">
			<extension url="http://hl7.org/fhir/uv/ips/StructureDefinition/conformance-cen-ips">
				<valueCode value="C"/>
			</extension>
			<path value="MedicalDevicesSection.ContentStatus"/>
			<short value="MedicalDevices content status"/>
			<definition value="The patient has had no previous MedicalDevices, or the MedicalDevice information is unknown."/>
			<min value="0"/>
			<max value="*"/>
			<type>
				<code value="CodedElement"/>
				<profile value="http://hl7.org/fhir/uv/ips/StructureDefinition/CodedElement-lm-cen-ips"/>
			</type>
		</element>
		<element id="MedicalDevicesSection.Devices">
			<extension url="http://hl7.org/fhir/uv/ips/StructureDefinition/conformance-cen-ips">
				<valueCode value="C"/>
			</extension>
			<path value="MedicalDevicesSection.Devices"/>
			<short value="Devices"/>
			<definition value="A conditional list of devices"/>
			<min value="0"/>
			<max value="*"/>
			<type>
				<code value="List"/>
				<profile value="http://hl7.org/fhir/uv/ips/StructureDefinition/List-lm-cen-ips"/>
			</type>
		</element>
		<element id="MedicalDevicesSection.Devices.Device">
			<extension url="http://hl7.org/fhir/uv/ips/StructureDefinition/conformance-cen-ips">
				<valueCode value="M"/>
			</extension>
			<path value="MedicalDevicesSection.Devices.Device"/>
			<short value="Device"/>
			<definition value="Each member (Device) of the list should have the same structure (i.e. type, identifier and use date)"/>
			<min value="1"/>
			<max value="*"/>
			<type>
				<code value="Device"/>
				<profile value="http://hl7.org/fhir/uv/ips/StructureDefinition/Device-lm-cen-ips"/>
			</type>
		</element>
	</differential>
</StructureDefinition>
