<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ../../../__FHIRr4Tools/fhir-all-xsd/structuredefinition.xsd">
	<id value="PregnancyHistorySection-lm-cen-ips"/>
	<url value="http://hl7.org/fhir/uv/ips/StructureDefinition/PregnancyHistorySection-lm-cen-ips"/>
	<name value="PregnancyHistorySection"/>
	<title value="History of Pregnancy Section (EN 17269)"/>
	<status value="draft"/>
	<description value="Considered to be generic to the English language and not specific to this standard."/>
	<purpose value="To present the current health state of the woman with respect to pregnancy and to provide chronological and outcome information about past pregnancies."/>
	<kind value="logical"/>
	<abstract value="true"/>
	<type value="Element"/>
	<baseDefinition value="http://hl7.org/fhir/StructureDefinition/Element"/>
	<derivation value="specialization"/>
	<differential>
		<element id="PregnancyHistorySection">
			<path value="PregnancyHistorySection"/>
			<short value="History of Pregnancy Section"/>
			<definition value="Present the current health state of the woman with respect to pregnancy and to provide chronological and outcome information about past pregnancies"/>
			<min value="0"/>
			<max value="*"/>
			<type>
				<code value="Section-lm-cen-ips"/>
				<profile value="http://hl7.org/fhir/uv/ips/StructureDefinition/Section-lm-cen-ips"/>
			</type>
			<constraint>
				<key value="br-1"/>
				<severity value="error"/>
				<human value="If pregnancy state is given then the expected delivery date SHALL be given."></human>
			</constraint>
			<constraint>
				<key value="br-2"/>
				<severity value="error"/>
				<human value="The description of the current Pregnancy SHALL be in Text or in a structured form, or both."></human>
			</constraint>
			<constraint>
				<key value="br-1"/>
				<severity value="error"/>
				<human value="If this IPS Section is included in the IPS Summary then it SHALL provide data concerning the current pregnancy status and optionally any information about previous pregnancies if known."></human>
			</constraint>
			<constraint>
				<key value="br-1"/>
				<severity value="error"/>
				<human value="If previous pregnancies are included then three possible ways of representing such information are given, i.e. Text or List of date and outcome pairs or a Summary metric.   One way SHALL be given but this does not exclude other descriptions being exchanged in the summary."/>
			</constraint>
		</element>
		<element id="PregnancyHistorySection.PregnancyStatus">
			<extension url="http://hl7.org/fhir/uv/ips/StructureDefinition/conformance-cen-ips">
				<valueCode value="R"/>
			</extension>
			<path value="PregnancyHistorySection.PregnancyStatus"/>
			<short value="Status of pregnancy"/>
			<min value="0"/>
			<max value="*"/>
			<type>
				<code value="LabelConcept-lm-cen-ips"/>
				<profile value="http://hl7.org/fhir/uv/ips/StructureDefinition/LabelConcept-lm-cen-ips"/>
			</type>
		</element>
		<element id="PregnancyHistorySection.KnownPreviousPregnancies">
			<extension url="http://hl7.org/fhir/uv/ips/StructureDefinition/conformance-cen-ips">
				<valueCode value="O"/>
			</extension>
