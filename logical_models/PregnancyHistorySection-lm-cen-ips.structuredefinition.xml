			<definition value="XXX"/>
			<min value="0"/>
			<max value="*"/>
			<type>
				<code value="LabelConcept-lm-cen-ips"/>
				<profile value="http://hl7.org/fhir/uv/ips/StructureDefinition/LabelConcept-lm-cen-ips"/>
			</type>
		</element>
		<element id="PregnancyHistorySection.PreviousPregnancies.Description">
			<extension url="http://hl7.org/fhir/uv/ips/StructureDefinition/conformance-cen-ips">
				<valueCode value="C"/>
			</extension>
			<path value="PregnancyHistorySection.PreviousPregnancies.Description"/>
			<short value="Previous pregnancies description"/>
			<definition value="The details of the current Pregnancy is either given as text or in a structured form, but not in both."/>
			<min value="0"/>
			<max value="*"/>
			<type>
				<code value="Text-lm-cen-ips"/>
				<profile value="http://hl7.org/fhir/uv/ips/StructureDefinition/Text-lm-cen-ips"/>
			</type>
		</element>
		<element id="PregnancyHistorySection.PreviousPregnancies.List">
			<extension url="http://hl7.org/fhir/uv/ips/StructureDefinition/conformance-cen-ips">
