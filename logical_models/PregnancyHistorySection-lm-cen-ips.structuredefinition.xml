			<type>
				<code value="HealthcareProvider-lm-cen-ips"/>
				<profile value="http://hl7.org/fhir/uv/ips/StructureDefinition/HealthcareProvider-lm-cen-ips"/>
			</type>
		</element>
	</differential>
</StructureDefinition>
