<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ../../../__FHIRr4Tools/fhir-all-xsd/structuredefinition.xsd">
	<id value="ProceduresSection-lm-cen-ips"/>
	<url value="http://hl7.org/fhir/uv/ips/StructureDefinition/ProceduresSection-lm-cen-ips"/>
	<name value="ProceduresSection"/>
	<title value="History of Procedures Section (EN 17269)"/>
	<status value="draft"/>
	<description value="Considered to be generic to the English language and not specific to this standard."/>
	<purpose value="A list of the procedures, and their descriptions, performed on the patient (procedure is generic and not just surgical)."/>
	<kind value="logical"/>
	<abstract value="true"/>
	<type value="Element"/>
	<baseDefinition value="http://hl7.org/fhir/StructureDefinition/Element"/>
	<derivation value="specialization"/>
	<differential>
		<element id="ProceduresSection">
			<path value="ProceduresSection"/>
			<short value="Procedures Section"/>
			<definition value="A list of the procedures, and their descriptions, performed on the patient (procedure is generic and not just surgical)."/>
			<min value="0"/>
			<max value="*"/>
			<type>
				<code value="Section-lm-cen-ips"/>
				<profile value="http://hl7.org/fhir/uv/ips/StructureDefinition/Section-lm-cen-ips"/>
			</type>
			<constraint>
				<key value="br-proc-1"/>
				<severity value="error"/>
				<human value="If content status positive then list of the procedures SHALL be non-empty AND the content status SHALL be omitted."/>
			</constraint>
			<constraint>
				<key value="br-proc-2"/>
				<severity value="error"/>
				<human value="If content status records absence then list of the procedures SHALL be omitted"/>
			</constraint>
		</element>
		<element id="ProceduresSection.ContentStatus">
			<extension url="http://hl7.org/fhir/uv/ips/StructureDefinition/conformance-cen-ips">
				<valueCode value="C"/>
			</extension>
			<path value="ProceduresSection.ContentStatus"/>
			<short value="Procedures content status"/>
			<definition value="History of procedures informing whether there is no information about procedures or no known procedure."/>
			<min value="0"/>
			<max value="*"/>
			<type>
				<code value="CodedElement-lm-cen-ips"/>
				<profile value="http://hl7.org/fhir/uv/ips/StructureDefinition/CodedElement-lm-cen-ips"/>
			</type>
		</element>
		<element id="ProceduresSection.Procedures">
			<extension url="http://hl7.org/fhir/uv/ips/StructureDefinition/conformance-cen-ips">
				<valueCode value="C"/>
			</extension>
			<path value="ProceduresSection.Procedures"/>
			<short value="Procedures"/>
			<definition value="A list of procedures performed on the subject of care."/>
			<min value="0"/>
			<max value="*"/>
			<type>
				<code value="List-lm-cen-ips"/>
				<profile value="http://hl7.org/fhir/uv/ips/StructureDefinition/List-lm-cen-ips"/>
			</type>
		</element>
		<element id="ProceduresSection.Procedures.Procedure">
			<extension url="http://hl7.org/fhir/uv/ips/StructureDefinition/conformance-cen-ips">
				<valueCode value="M"/>
			</extension>
			<path value="ProceduresSection.Procedures.Procedure"/>
			<short value="Procedure"/>
			<definition value="Each member of the list describes the type of procedure and identifies the actual procedure performed and when it happened."/>
			<min value="1"/>
			<max value="*"/>
			<type>
				<code value="Procedure"/>
				<profile value="http://hl7.org/fhir/uv/ips/StructureDefinition/Procedure-lm-cen-ips"/>
			</type>
		</element>
	</differential>
</StructureDefinition>
